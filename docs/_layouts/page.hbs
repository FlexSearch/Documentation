{{> head }}
<body>
    {{> navbar }}
    <section class="section-primary no-print searchbox">
        <div class="container">
            <div class="row">
                <div class="col-sm-12">
                    <input id="searchbox" class="awesomplete" type="text" name="q" placeholder="Search the documentation">
                    <div id="search-results" style="position:relative;background:white"></div>
                </div>
            </div>
        </div>
    </section>
    <section class="section-main">
        <div class="container">
            <div class="row">
                <main class="col-sm-12 col-md-8 offset-md-2" role="main">
                    {{#compare page.order 100}}
                    <div class="print-only-section-header">
                        <h1 id="{{page.area}}" class="uppercase spaced-title">{{page.area}}</h1>
                    </div>
                    {{/compare}}
                    <div class="page-break"></div>
                    <header class="no-print"><h1 id="{{page.id}}" class="uppercase spaced-title">{{page.title}}</h1></header>
                    <header class="print-only-section-header"><h2 id="{{page.id}}" class="uppercase spaced-title">{{page.title}}</h2></header>
                    {{> toc}}
                    <article>
                    {{{content}}}
                    </article>
                </main>
            </div>
        </div>
    </section>
{{> footer }}
</body>
</html>